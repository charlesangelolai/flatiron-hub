<h4 class="btn-flat cyan accent-4 white-text"><i class="fa fa-calendar" aria-hidden="true"></i>&nbsp;&nbsp;Cohort: <%= @cohort.name %></h4>
<br><br>
<div class="row">
  <div class="col s5">
    <img style="height: 300px; width: 350px;" src="/images/community.svg" alt="">
  </div>
  <div class="col s7">
    <div class="card cyan accent-4">
      <div class="card-content white-text">
        <span class="card-title" style="padding-bottom: 15px;"><i class="fa fa-users" aria-hidden="true"></i>&nbsp;&nbsp;Students: </span>
        <ul class="collection">
          <% @students.each do |student| %>
            <li><a class="collection-item" href="/profile/<%= student.username %>"><%= student.first_name %> <%= student.last_name %></a></li>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
</div>