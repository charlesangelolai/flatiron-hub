<h4 class="btn-flat cyan accent-4 white-text">Cohort: <%= @students.first.cohort_id %></h4>
<br><br>
<div class="row">
  <div class="col s5">
    <img style="height: 300px; width: 300px;" src="/images/profile_pic.svg" alt="">
  </div>
  <div class="col s7">
    <div class="card cyan accent-4">
      <div class="card-content white-text">
        <span class="card-title">Students</span>
        <ul>
          <% @students.each do |student| %>
            <li><a class="btn-small" href="/profile/<%= student.username %>"><%= student.first_name %> <%= student.last_name %></a></li>
            <br>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
</div>